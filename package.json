  "description": "Lightweight gulp plugin that adds !important to all you css rules.",
  "keywords": [
    "gulpplugin",
    "css",
    "reset",
    "important",
    "widget",
    "vip"
  ],
  "author": {
    "name": "David Gorges",
    "email": "david.gorges@leaphub.de",
    "url": "https://github.com/davidgorges"
  },
